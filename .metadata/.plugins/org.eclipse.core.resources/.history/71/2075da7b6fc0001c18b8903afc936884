package com.gd.day3;

//Public class name and file name should be equal
//one file can have multiple class in java file
class Person {

	
	private String name;
	private int age;
	private String degree;
	
	// variable
	// static
	// final
	// default
	// public ,private , protected , default

	// method
	// --->main method
	// ---->constructor
	// ---->setter and getter
	// -->public ,private , protected , default
	// static
		public void sayName()
		{
			System.out.println(this.name);
		}
	
	
}

public class MyClass {

	public static void main(String[] args) {
		
		Person Ravinder=new Person();
		Ravinder.name="Ravinder";
		Ravinder.age=35;
		Ravinder.degree="M.Tech";
		
		
		System.out.println(Ravinder.name);
		System.out.println(Ravinder.age);
		System.out.println(Ravinder.degree);
	}
}
