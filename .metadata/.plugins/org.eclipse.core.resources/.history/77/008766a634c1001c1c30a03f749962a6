package com.gd.day4;

public class Employee {

	
	int id;
	String name;
	double salary;
	String deptid;
	
	
	
	
	public static void main(String[] args) {
		
		//class name      object name      //new  constructor
		Employee         employee       =    new Employee();
		
		employee.equals(employee);
		employee.hashCode();
		employee.toString();
		employee.notify();
	}
}
