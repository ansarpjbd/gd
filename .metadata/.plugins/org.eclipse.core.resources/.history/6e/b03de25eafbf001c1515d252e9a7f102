package com.gd.day2;

public class MyLooping {
	
	public static void main(String[] args) {
		
		
		//Looping --it is used when we have to repeat some link of code again anf again
		
		int a=1;
		
		System.out.println(a);
		a=a+1;  // add the one to last value
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		a=a+1;
		System.out.println(a);
		
		
		
	}

}
