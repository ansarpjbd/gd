package com.gd.day6;

//Micromax

//mobile
//

abstract class Mobile {
	// you can have the abstract and non abstract method
	// it will extends by another class
	public void calling() {
		System.out.println("calling");
	}

	public void sendsms() {
		System.out.println("send sms");
	}
	
	abstract public void brand();

}

public class MYAbstractClass {

}
